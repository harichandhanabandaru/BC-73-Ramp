{
  "compilerOptions": {
    "outDir": "frontend/dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "src*": ["./src/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"]
}
